<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admissionguide">
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AdmissionGuide">
        <activity android:name=".login"></activity>
        <activity android:name=".contact" />
        <activity android:name=".facilities" />
        <activity android:name=".research" />
        <activity android:name=".sports" />
        <activity android:name=".library" />
        <activity android:name=".alumni" />
        <activity android:name=".placement" />
        <activity android:name=".coe" />
        <activity android:name=".admission" />
        <activity android:name=".academics" />
        <activity android:name=".accomodations" />
        <activity android:name=".about" />
        <activity android:name=".department" />
        <activity android:name=".navigation" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>